   "source": [
    "#| export\n",
    "def mk_comp(name, def_cls):\n",
    "    comp = getattr(fc, name)\n",
    "    globals()[name] = lambda *args, var='', cls='', **kwargs: comp(*args, cls=f'{def_cls}{\"-\"+var if var else \"\"} {cls}', **kwargs)"
   ]

    "### Render Existing Messages"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "764bf26a",
   "metadata": {
    "time_run": "2026-01-22T00:39:42.067948+00:00"
   },
   "outputs": [],
   "source": [
    "#| export\n",
    "@rt\n",
    "def load_messages(session):\n",
    "    c,sh = get_chat(session)\n",
    "    for i in c.hist:\n",
    "        role = i.get('role') if isinstance(i, dict) else i.role\n",
    "        if role=='user':      yield Div(Bubble(i['content'].replace('[INTERPRETER INPUT]\\n', ''), i['type']), cls='space-y-0.5')\n",
    "        if role=='assistant': yield Div(Bubble(i['content'].replace('[INTERPRETER OUTPUT]\\n', ''), i['type'], 'r'), cls='space-y-0.5')"
   ]
  },

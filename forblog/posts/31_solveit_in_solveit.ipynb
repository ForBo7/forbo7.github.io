    "@rt\n",
    "def toggle_lang(session):\n",
    "    lang = 'zh' if session.get('lang') == 'en' else 'en'\n",
    "    current_lang.set(lang)\n",
    "    session['lang'] = lang\n",
    "    return (\n",
    "        H2(t('solveit_lite'), id='title', hx_swap_oob='true'),\n",

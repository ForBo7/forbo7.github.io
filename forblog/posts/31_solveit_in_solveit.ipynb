    {
     "data": {
      "text/plain": [
       "{(1, 1): (<lisette.core.Chat at 0x70ee1a6efda0>,\n",
       "  <IPython.terminal.interactiveshell.TerminalInteractiveShell at 0x70ee225f31a0>)}"
      ]
     },
     "execution_count": 0,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "ensure_dialog(fake_session)\n",
    "user_chats"
   ]
